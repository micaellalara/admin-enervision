            <div class="main-panel">
                <div class="content-wrapper">
                    <div class="header d-flex align-items-center justify-content-between mb-4">
                        <div class="d-flex align-items-center">
                            <img src="/images/devManagement.png" style="width: 100px; height: 50px;" class="header-icon"
                                alt="">
                            <h2 class="ml-2" style="font-size: 30px;">Appliance Management</h2>
                        </div>

                        <button type="button" class="btn btn-primary mb-3" data-toggle="modal"
                            data-target="#addDeviceModal" style="width: 20%;">
                            <i class="fas fa-plus-circle mr-2" style="color: white;"></i> Add New Appliance
                        </button>

                    </div>
                    <div class="modal fade" id="addDeviceModal" tabindex="-1" role="dialog"
                        aria-labelledby="addDeviceModalLabel" aria-hidden="true">
                        <div class="modal-dialog" role="document">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <h5 class="modal-title" id="addDeviceModalLabel">Appliance Details</h5>
                                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                        <span aria-hidden="true">&times;</span>
                                    </button>
                                </div>
                                <form id="addDeviceForm" action="/devices" method="POST">
                                    <div class="modal-body">
                                        <div class="form-group">
                                            <label for="deviceName">Appliance Name</label>
                                            <input type="text" class="form-control" id="deviceName" name="deviceName"
                                                required>
                                        </div>
                                        <div class="form-group">
                                            <label for="description">Description</label>
                                            <textarea class="form-control" id="description"
                                                name="description"></textarea>
                                        </div>
                                        <div class="form-group">
                                            <label for="capacity">Capacity</label>
                                            <input type="text" class="form-control" id="capacity" name="capacity"
                                                required>
                                        </div>
                                        <div class="form-group">
                                            <label for="material">Material</label>
                                            <input type="text" class="form-control" id="material" name="material"
                                                required>
                                        </div>
                                        <div class="form-group">
                                            <label for="purchasePrice">Purchase Price</label>
                                            <input type="number" class="form-control" id="purchasePrice"
                                                name="purchasePrice" step="0.01" required>
                                        </div>
                                        <div class="form-group">
                                            <label for="powerConsumption">Power Consumption</label>
                                            <input type="text" class="form-control" id="powerConsumption"
                                                name="powerConsumption" required>
                                        </div>
                                        <div class="form-group">
                                            <label for="costPerHour">Cost Per Hour</label>
                                            <input type="number" class="form-control" id="costPerHour"
                                                name="costPerHour" step="0.01" required>
                                        </div>
                                        <div class="form-group">
                                            <label for="monthlyCost">Monthly Cost</label>
                                            <input type="text" class="form-control" id="monthlyCost" name="monthlyCost"
                                                required>
                                        </div>
                                        <div class="form-group">
                                            <label for="applianceCategory">Category</label>
                                            <select class="form-control" id="applianceCategory" name="applianceCategory"
                                                required>
                                                <option value="Lighting">Lighting</option>
                                                <option value="Entertainment">Entertainment</option>
                                                <option value="Cooking">Cooking</option>
                                                <option value="Cooling">Cooling</option>
                                                <option value="Laundry">Laundry</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="modal-footer">
                                        <button type="button" class="btn btn-secondary"
                                            data-dismiss="modal">Close</button>
                                        <button type="submit" class="btn btn-primary">Save Appliance</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>

                    <br>

                    <div class="table-responsive" style="max-height: 450px; overflow-y: auto;">
                        <% if (devices.length===0) { %>
                            <p>No devices available.</p>
                            <% } else { %>
                                <table class="table table-bordered">
                                    <thead class="thead-light" style="text-align: center; vertical-align: middle;">
                                        <tr>
                                            <th>Appliance Name</th>
                                            <th>Capacity</th>
                                            <th>Material</th>
                                            <th>Purchase Price</th>
                                            <th>Power Consumption</th>
                                            <th>Cost Per Hour</th>
                                            <th>Monthly Cost</th>
                                            <th>Category</th>
                                            <th>Description</th>
                                            <th>Actions</th>
                                        </tr>
                                    </thead>
                                    <tbody style="text-align: center; vertical-align: middle;">
                                        <% devices.forEach(device=> { %>
                                            <tr data-device-id="<%= device._id %>">
                                                <td>
                                                    <%= device.deviceName %>
                                                </td>
                                                <td>
                                                    <%= device.capacity %>
                                                </td>
                                                <td>
                                                    <%= device.material %>
                                                </td>
                                                <td>
                                                    <%= device.purchasePrice %>
                                                </td>
                                                <td>
                                                    <%= device.powerConsumption %>
                                                </td>
                                                <td>
                                                    <%= device.costPerHour %>
                                                </td>
                                                <td>
                                                    <%= device.monthlyCost %>
                                                </td>
                                                <td>
                                                    <%= device.applianceCategory %>
                                                </td>
                                                <td>
                                                    <%= device.description %>
                                                </td>
                                                <td>
                                                    <button class="btn btn-warning btn-sm"
                                                        onclick="showEditDeviceModal('<%= device._id %>')">Edit</button>
                                                    <button class="btn btn-danger btn-sm"
                                                        onclick="showDeleteDeviceModal('<%= device._id %>')">Delete</button>
                                                </td>
                                            </tr>
                                            <% }) %>
                                    </tbody>
                                </table>
                                <% } %>
                    </div>
                </div>
            </div>

            <div class="modal fade" id="editDeviceModal" tabindex="-1" role="dialog"
                aria-labelledby="editDeviceModalLabel" aria-hidden="true">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="editDeviceModalLabel">Edit Appliance Details</h5>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                        <div class="modal-body">
                            <form id="editDeviceForm">
                                <input type="hidden" id="editDeviceId">
                                <div class="form-group">
                                    <label for="editDeviceName">Appliance Name</label>
                                    <input type="text" id="editDeviceName" class="form-control" required>
                                </div>
                                <div class="form-group">
                                    <label for="editCapacity">Capacity</label>
                                    <input type="text" id="editCapacity" class="form-control" required>
                                </div>
                                <div class="form-group">
                                    <label for="editMaterial">Material</label>
                                    <input type="text" id="editMaterial" class="form-control" required>
                                </div>
                                <div class="form-group">
                                    <label for="editPurchasePrice">Purchase Price</label>
                                    <input type="number" id="editPurchasePrice" class="form-control" required>
                                </div>
                                <div class="form-group">
                                    <label for="editPowerConsumption">Power Consumption</label>
                                    <input type="text" id="editPowerConsumption" class="form-control" required>
                                </div>
                                <div class="form-group">
                                    <label for="editCostPerHour">Cost Per Hour</label>
                                    <input type="number" id="editCostPerHour" class="form-control" step="0.01" required>
                                </div>
                                <div class="form-group">
                                    <label for="editMonthlyCost">Monthly Cost</label>
                                    <input type="text" id="editMonthlyCost" class="form-control" required>
                                </div>
                                <div class="form-group">
                                    <label for="editApplianceCategory">Appliance Category</label>
                                    <select id="editApplianceCategory" class="form-control" required>
                                        <option value="Lighting">Lighting</option>
                                        <option value="Entertainment">Entertainment</option>
                                        <option value="Cooking">Cooking</option>
                                        <option value="Cooling">Cooling</option>
                                        <option value="Laundry">Laundry</option>
                                    </select>
                                </div>
                                <button type="submit" class="btn btn-primary">Save Updates</button>
                            </form>
                        </div>
                    </div>
                </div>
            </div>

            <div class="modal fade" id="deleteDeviceModal" tabindex="-1" role="dialog"
                aria-labelledby="deleteDeviceModalLabel" aria-hidden="true">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="deleteDeviceModalLabel">Delete Appliance</h5>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                        <div class="modal-body">
                            <p>Are you sure you want to delete this appliance?</p>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
                            <button type="button" class="btn btn-danger" id="deleteDeviceButton">Delete</button>
                        </div>
                    </div>
                </div>
            </div>

            <script>
                function showEditDeviceModal(deviceId) {
                    fetch(`/device/${deviceId}`)
                        .then(response => response.json())
                        .then(device => {
                            document.getElementById("editDeviceId").value = device._id;
                            document.getElementById("editDeviceName").value = device.deviceName;
                            document.getElementById("editCapacity").value = device.capacity;
                            document.getElementById("editMaterial").value = device.material;
                            document.getElementById("editPurchasePrice").value = device.purchasePrice;
                            document.getElementById("editPowerConsumption").value = device.powerConsumption;
                            document.getElementById("editCostPerHour").value = device.costPerHour;
                            document.getElementById("editMonthlyCost").value = device.monthlyCost;
                            document.getElementById("editApplianceCategory").value = device.applianceCategory;

                            // Show the modal
                            $('#editDeviceModal').modal('show');
                        });
                }

                document.getElementById("editDeviceForm").onsubmit = function (event) {
                    event.preventDefault();
                    const deviceId = document.getElementById("editDeviceId").value;
                    const data = {
                        deviceName: document.getElementById("editDeviceName").value,
                        capacity: document.getElementById("editCapacity").value,
                        material: document.getElementById("editMaterial").value,
                        purchasePrice: document.getElementById("editPurchasePrice").value,
                        powerConsumption: document.getElementById("editPowerConsumption").value,
                        costPerHour: document.getElementById("editCostPerHour").value,
                        monthlyCost: document.getElementById("editMonthlyCost").value,
                        applianceCategory: document.getElementById("editApplianceCategory").value
                    };

                    fetch(`/devices/edit/${deviceId}`, {
                        method: 'PUT',
                        headers: {
                            'Content-Type': 'application/json'
                        },
                        body: JSON.stringify(data)
                    })
                        .then(response => response.json())
                        .then(updatedDevice => {
                            const row = document.querySelector(`tr[data-device-id='${deviceId}']`);
                            row.querySelector('td:nth-child(1)').textContent = updatedDevice.deviceName;
                            row.querySelector('td:nth-child(2)').textContent = updatedDevice.capacity;
                            row.querySelector('td:nth-child(3)').textContent = updatedDevice.material;
                            row.querySelector('td:nth-child(4)').textContent = updatedDevice.purchasePrice;
                            row.querySelector('td:nth-child(5)').textContent = updatedDevice.powerConsumption;
                            row.querySelector('td:nth-child(6)').textContent = updatedDevice.costPerHour;
                            row.querySelector('td:nth-child(7)').textContent = updatedDevice.monthlyCost;
                            row.querySelector('td:nth-child(8)').textContent = updatedDevice.applianceCategory;
                            row.querySelector('td:nth-child(9)').textContent = updatedDevice.description;

                            $('#editDeviceModal').modal('hide');
                        });
                };

                function showDeleteDeviceModal(deviceId) {
                    $('#deleteDeviceModal').modal('show');
                    document.getElementById("deleteDeviceButton").onclick = function () {
                        fetch(`/devices/delete/${deviceId}`, {
                            method: 'DELETE'
                        })
                            .then(response => response.json())
                            .then(() => {
                                const row = document.querySelector(`tr[data-device-id='${deviceId}']`);
                                row.remove();

                                $('#deleteDeviceModal').modal('hide');
                            });
                    };
                }
            </script>

